{
  "name": "Chat-Application",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm-run-all --parallel serve:dev:server serve:dev:client",
    "start:prod": "npm run schema:restore && npm run serve:prod",
    "deploy": "npm run schema:dump && npm run schema:commit && git push deploy -f",
    "build:dev": "webpack",
    "build:prod": "NODE_ENV=production webpack",
    "serve:dev:client": "hjs-dev-server",
    "serve:dev:server": "hz serve --dev",
    "serve:prod": "node ./util/server/express.js",
    "schema:commit": "git add ./.hz/schema.toml >/dev/null || true && git commit -m \"Updated DB Schema\" ./.hz/schema.toml >/dev/null || true",
    "schema:dump": "hz get-schema -o ./.hz/schema.toml",
    "schema:restore": "hz set-schema ./.hz/schema.toml --connect $(./util/scripts/parseurl.sh $RETHINKDB_URL)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tim2pin/Chat-Application.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tim2pin/Chat-Application/issues"
  },
  "homepage": "https://github.com/tim2pin/Chat-Application#readme",
  "dependencies": {
    "@horizon/client": "^1.1.3",
    "browserify": "^13.1.0",
    "elemental": "^0.6.0",
    "express": "^4.14.0",
    "hjs-webpack": "^8.3.0",
    "horizon": "^1.1.3",
    "jsx-loader": "^0.13.2",
    "less": "^2.7.1",
    "npm-run-all": "^2.3.0",
    "react": "^15.3.0",
    "react-addons-css-transition-group": "^15.3.0",
    "react-count": "^0.2.4",
    "react-dom": "^15.3.0",
    "react-router": "^2.6.1",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "jsx-loader": "^0.13.2",
    "less-loader": "^2.2.3",
    "postcss-loader": "^0.9.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7"
  }
}
